<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="593.0" prefWidth="440.0" style="-fx-background-color: #f8f6ee;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddCustomerFormController">
   <children>
      <JFXTextField fx:id="txtCustomerId" labelFloat="true" layoutX="49.0" layoutY="43.0" onAction="#moveToCustomerTitle" prefHeight="26.0" prefWidth="342.0" promptText="Customer Id" />
      <JFXTextField fx:id="txtCustomerTitle" labelFloat="true" layoutX="49.0" layoutY="110.0" onAction="#moveToCustomerName" prefHeight="26.0" prefWidth="342.0" promptText="Customer Title" />
      <JFXTextField fx:id="txtCustomerName" labelFloat="true" layoutX="49.0" layoutY="175.0" onAction="#moveToCustomerAddress" prefHeight="26.0" prefWidth="342.0" promptText="Customer Name" />
      <JFXTextField fx:id="txtCity" labelFloat="true" layoutX="49.0" layoutY="299.0" onAction="#moveToCustomerProvince" prefHeight="26.0" prefWidth="342.0" promptText="City" />
      <JFXTextField fx:id="txtProvince" labelFloat="true" layoutX="49.0" layoutY="372.0" onAction="#moveToCustomerPostelCode" prefHeight="26.0" prefWidth="342.0" promptText="Province" />
      <JFXTextField fx:id="txtPostelCode" labelFloat="true" layoutX="49.0" layoutY="437.0" prefHeight="26.0" prefWidth="342.0" promptText="Postel Code" />
      <JFXTextField fx:id="txtCustomerAddress" labelFloat="true" layoutX="49.0" layoutY="237.0" onAction="#moveToCustomerCity" prefHeight="26.0" prefWidth="342.0" promptText="Customer Address" />
      <JFXButton buttonType="RAISED" layoutX="49.0" layoutY="523.0" onAction="#addCustomerOnAction" prefHeight="31.0" prefWidth="136.0" style="-fx-background-color: #1abc9c;" text="Add Customer" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </JFXButton>
      <JFXButton fx:id="cancelContext" buttonType="RAISED" layoutX="293.0" layoutY="523.0" onAction="#cancleOnAction" prefHeight="31.0" prefWidth="98.0" style="-fx-background-color: #e74c3c;" text="Cancle" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </JFXButton>
   </children>
</AnchorPane>
